---
layout: null
---
{
	"code" : 200 ,
	"data" : [
		{% for post in site.posts %}
			{
				"url" : "{{ post.url }}",
				"title" : "{{ post.title }} - {% for tag in post.tags %}{% if forloop.rindex != 1 %}{{ tag }}_{% else %}{{ tag }}{% endif %}{% endfor %}",
				"author": "{{ post.author }}",
				"date": "{{ post.date | date: date_format }}",
				"content": "{{ post.content | strip_html }}"
			}
			{% if forloop.rindex != 1  %}
			,
			{% endif %}
		{% endfor %}
	]
}